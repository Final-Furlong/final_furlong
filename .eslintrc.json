{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "requireConfigFile": false
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "globals": {},
  "ignorePatterns": ["/**/test/**/*.js"],
  "rules": {
    "prettier/prettier": "error"
  }
}
